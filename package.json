{
  "name": "sourcerer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel testApp:watch testServer:watch open:src lint:watch",
    "open:src": "babel-node tools/server/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "testApp": "mocha --reporter progress tools/tests/testSetup.js \"app/**/*.test.js\"",
    "testApp:watch": "npm run testApp -- --watch",
    "testServer": "mocha --reporter progress tools/tests/testSetup.js \"tools/**/*.test.js\"",
    "testServer:watch": "npm run testServer -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist testApp lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"

  },
  "author": "",
  "license": "ISC",
